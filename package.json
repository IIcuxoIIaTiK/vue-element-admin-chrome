    "ext": "npm run ext-build",
    "build": "npm run ext-build",
    "dev": "npm run ext-dev",
    "run": "npm run ext-run",
    "ext-build": "webpack --config ./build/webpack.ext.prod.js -p --progress --hide-modules --colors",
    "ext-dev": "webpack --config ./build/webpack.ext.dev.js --display-error-details --debug --colors --hide-modules",
    "ext-run": "webpack --config ./build/webpack.ext.dev.js --hide-modules",
    "ext-build-ssl": "node build/build.js",
    "web": "npm run web-dev",
    "web-dev": "webpack-dev-server --debug --colors --inline --progress --config ./build/web/webpack.dev.conf.js",
    "web-dist": "npm run web-build",
    "web-build": "node ./build/web/build.js",
    "web-build:report": "npm_config_report=true node ./build/web/build.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint",
    "archive": "zip -q -r tarball/extension-all.zip dist",
    "clear": "rm -rf tarball dist"
